  "context_disambiguation_rules": [
    {
      "pattern": "{}/2 = 0.5",
      "likely": "1",
      "unlikely": ["l", "I"],
      "reason": "Arithmetic context"
    },
    {
      "pattern": "\\sum_{i={}}^{n}",
      "likely": "0",
      "unlikely": ["O", "o"],
      "reason": "Summation lower bound usually 0 or 1"
    },
    {
      "pattern": "x^{}",
      "likely": "2",
      "unlikely": ["z"],
      "reason": "Squared is most common exponent"
    },
    {
      "pattern": "{} \\in \\mathbb{R}",
      "likely": "x",
      "unlikely": ["×", "χ"],
      "reason": "Element-of expects variable, not operator"
    },
    {
      "pattern": "2 {} 3 = 6",
      "likely": "×",
      "unlikely": ["x", "χ"],
      "reason": "Multiplication context"
    },
    {
      "pattern": "f({}) = 1",
      "likely": "0",
      "unlikely": ["O", "o"],
      "reason": "Function evaluation often at 0"
    },
    {
      "pattern": "\\sin({}) = 0",
      "likely": "0",
      "unlikely": ["O", "θ"],
      "reason": "sin(0) = 0 is common"
    }
  ]
